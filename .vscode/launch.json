      "module": "autocorpus",
      "args": [
        "-b",
        "PMC",
        "-t",
        "output",
        "-f",
        "${workspaceFolder}/tests/data/public/html/PMC/PMC8885717.html"
      ]
    },
    {
      "name": "Run Auto-CORPus with legacy PMC config",
      "type": "debugpy",
      "request": "launch",
      "module": "autocorpus",
      "args": [
        "-b",
        "LEGACY_PMC",
        "-t",
        "output",
        "-f",
        "${workspaceFolder}/tests/data/public/html/LEGACY_PMC/PMC8885717.html"
      ]
    }
  ]
