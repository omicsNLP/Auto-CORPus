  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Auto-CORPus with PMC config",
      "type": "debugpy",
      "request": "launch",
      "module": "autocorpus",
      "args": [
        "-c",
        "${workspaceFolder}/autocorpus/configs/config_pmc.json",
        "-t",
        "output",
        "-f",
        "${workspaceFolder}/tests/data/html/PMC/PMC8885717.html"
      ]
    },
    {
      "name": "Run Auto-CORPus with legacy PMC config",
      "type": "debugpy",
      "request": "launch",
      "module": "autocorpus",
      "args": [
        "-c",
        "${workspaceFolder}/autocorpus/configs/config_pmc_pre_oct_2024.json",
        "-t",
        "output",
        "-f",
        "${workspaceFolder}/tests/data/html/LEGACY_PMC/PMC8885717.html"
      ]
    }
  ]
}
